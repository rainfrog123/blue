## Data Download Commands
freqtrade download-data --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/download.json --timerange 20250801- --timeframes 1m 3m 5m 15m 30m 1h 4h --datadir /allah/freqtrade/user_data/data/binance

# --timerange 1752105600-1752109200

freqtrade backtesting --strategy TemaSlope --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/backtest.json --timerange 1761501600-1761505200 --datadir /allah/freqtrade/user_data/data/binance --cache none --starting-balance 10000 --eps --fee 0

freqtrade backtesting --strategy TemaSlope --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/backtest.json --timerange 20251101- --datadir /allah/freqtrade/user_data/data/binance --cache none --starting-balance 10000 --eps --fee 0

# Hyperopt to find best ATR SL multiplier (k), TP is auto 2x SL
freqtrade hyperopt --strategy TemaSlope --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/backtest.json --timerange 1761501600-1761505200 --datadir /allah/freqtrade/user_data/data/binance --hyperopt-loss SharpeHyperOptLossDaily --spaces sell -e 100 --fee 0

freqtrade trade --strategy TestExecution --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/live_spread_capture.json

freqtrade trade --strategy SpreadCapture --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/live_spread_capture.json

freqtrade webserver --userdir /allah/blue/freq/project/user_data --config /allah/blue/freq/project/user_data/config/backtest.json --datadir /allah/freqtrade/user_data/data/binance

